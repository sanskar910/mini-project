{
  "name": "faculty-timetable-management",
  "version": 2,
  "builds": [
    {
      "src": "my-react-project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/my-react-project/$1"
    }
  ]
}